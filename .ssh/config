Host *
    ServerAliveInterval 240

Host gitlab.magicleap.io
  HostName gitlab-ssh.magicleap.io
  ProxyCommand /usr/local/bin/cloudflared access ssh --hostname %h

Host github.magicleap.com
  HostName github-ssh.magicleap.com
  ProxyCommand /usr/local/bin/cloudflared access ssh --hostname %h

Host gerrit.magicleap.com
  HostName gerrit-ssh.magicleap.com
  ProxyCommand /usr/local/bin/cloudflared access ssh --hostname %h

Host artifactory.magicleap.com
  HostName artifactory-ssh.magicleap.com
  ProxyCommand /usr/local/bin/cloudflared access ssh --hostname %h
